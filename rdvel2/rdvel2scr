#!/bin/sh
# script to run rdvel
cp ~/binsyn/ia/RDVEL2.INF ./
cp ~/binsyn/oa/PGAX2.DAT ./
cp ~/binsyn/oa/PGBX2.DAT ./
cp ~/binsyn/oa/PGBX3.DAT ./
cp ~/binsyn/ia/CALPT.INF ./
cp ~/binsyn/oa/PGCX2.DAT ./
cp ~/binsyn/oa/ACPGA2.DAT ./
cp ~/binsyn/oa/ACPGBX2.DAT ./
cp ~/binsyn/oa/ACPGCX2.DAT ./
cp ~/binsyn/oa/PGDX4.DAT ./
cp ~/binsyn/oa/PGDX7.DAT ./
./rdvel2
cp RDVL1.DAT ~/binsyn/oa/
cp RDVL2.DAT ~/binsyn/oa/
cp RDVL3.DAT ~/binsyn/oa/
cp RDVL4.DAT ~/binsyn/oa/
cp RDVL5.DAT ~/binsyn/oa/
cp RDVL6.DAT ~/binsyn/oa/
cp RDVL7.DAT ~/binsyn/oa/
cp RDVL8.DAT ~/binsyn/oa/
cp RDVL9.DAT ~/binsyn/oa/

