#!/bin/sh -e
# script to run datconv
#NOTE: The following two lines have been commented out to permit
#program synacsp to run efficiently. The following two files
#must be copied into the datconv directory before starting a run.
#cp ../oa/ACPGF61.DAT ./
#cp ../oa/PGBX3.DAT ./
cp ../ia/DATCONV.INF ./
./datconv
cp SYSSM.DAT ../oa/SYSSM.DAT
cp SYSC1.DAT ../oa/SYSC1.DAT
cp SYSC2.DAT ../oa/SYSC2.DAT
cp SYSFC.DAT ../oa/SYSFC.DAT
cp SYSRM.DAT ../oa/SYSRM.DAT
cp SYSSC.DAT ../oa/SYSSC.DAT
cp SYSSM.DAT ~/plotfiles/SYSSM.DAT
cp SYSC1.DAT ~/plotfiles/SYSC1.DAT
cp SYSC2.DAT ~/plotfiles/SYSC2.DAT
cp SYSFC.DAT ~/plotfiles/SYSFC.DAT
cp SYSRM.DAT ~/plotfiles/SYSRM.DAT


